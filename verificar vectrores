 #!/bin/bash
vectores($url)
{ 
   login= $(wget $url | grep "login")
   coment= $(wget $url | grep"comment")
   filter= $(wget $url | grep"filter") 
   search= $(wget $url | grep"search")
   category= $(wget $url | grep"comment")
   if [ -e $login ]
      diccionario=true
   if [ -e $filter ] || [-e $category ]
      sql=true
   if [ -e $comment ] || [ -e $search ] 
      xss=true
}
